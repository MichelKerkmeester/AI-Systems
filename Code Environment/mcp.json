{
 "mcpServers": {
  "chrome-devtools-1": {
   "command": "npx",
   "args": [
    "chrome-devtools-mcp@latest",
    "--isolated=true"
   ],
   "env": {},
   "working_directory": null,
   "disabled": false,
   "alwaysAllow": []
  },
  "chrome-devtools-2": {
   "command": "npx",
   "args": [
    "chrome-devtools-mcp@latest",
    "--isolated=true"
   ],
   "env": {},
   "working_directory": null,
   "disabled": true,
   "alwaysAllow": []
  },
  "semantic-search": {
   "command": "/Users/USERNAME/mcp-servers/semantic-search/venv/bin/python",
   "args": [
    "/Users/USERNAME/mcp-servers/semantic-search/src/server_sqlite.py"
   ],
   "env": {
    "MCP_CODEBASE_WORKSPACE": "/Users/USERNAME/projects/your-project",
    "MCP_CODEBASE_DB_PATH": "/Users/USERNAME/projects/your-project/.codebase/vectors.db",
    "MCP_CODEBASE_EMBEDDER_PROVIDER": "openai-compatible",
    "MCP_CODEBASE_EMBEDDER_API_KEY": "YOUR_VOYAGE_API_KEY_HERE",
    "MCP_CODEBASE_EMBEDDER_BASE_URL": "https://api.voyageai.com/v1",
    "MCP_CODEBASE_EMBEDDER_MODEL_ID": "voyage-code-3",
    "MCP_CODEBASE_JUDGE_PROVIDER": "openai-compatible",
    "MCP_CODEBASE_JUDGE_API_KEY": "YOUR_VOYAGE_API_KEY_HERE",
    "MCP_CODEBASE_JUDGE_BASE_URL": "https://api.voyageai.com/v1",
    "MCP_CODEBASE_JUDGE_MODEL_ID": "voyage-3",
    "MCP_CODEBASE_JUDGE_MAX_TOKENS": "1024",
    "MCP_CODEBASE_JUDGE_TEMPERATURE": "0"
   },
   "working_directory": null,
   "disabled": false,
   "alwaysAllow": []
  }
 }
}